<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    tools:context=".fragment.Fragment_me"
    android:background="@color/white">
<!--      发现  -->
    <RelativeLayout
        android:id="@+id/top_find"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="vertical"
        android:background="#E9E9E9"
        >
        <TextView
            android:id="@+id/find_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="13dp"
            android:text="发现"
            android:textColor="@color/black"
            android:textSize="18dp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_add"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:layout_alignTop="@id/find_text"
            android:background="@drawable/bg_youchat_add"
            />
        <Button
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginRight="15dp"
            android:layout_alignTop="@id/find_text"
            android:layout_toLeftOf="@id/btn_add"
            android:background="@drawable/bg_youchat_search"

            >
        </Button>

    </RelativeLayout>
    <!--  朋友圈  -->
    <RelativeLayout
        android:clickable="true"
        android:background="@drawable/bg_all"
        android:layout_width="match_parent"
        android:layout_height="54dp"
        >

        <ImageView
            android:id="@+id/find_moments"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/bg_find_moments" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:layout_toRightOf="@id/find_moments"
            android:text="朋友圈"
            android:textColor="@color/black"
            android:textSize="19dp" />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:src="@drawable/arrows" />

    </RelativeLayout>
    <View
        android:id="@+id/thickLine1"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#E9E9E9"/>
    <!--    扫一扫-->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:clickable="true"
        android:background="@drawable/bg_all"
        >

        <ImageView
            android:id="@+id/find_scan"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/bg_find_scan" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:layout_toRightOf="@id/find_scan"
            android:text="扫一扫"
            android:textColor="@color/black"
            android:textSize="19dp" />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:src="@drawable/arrows" />

    </RelativeLayout>

    <View
        android:layout_marginLeft="65dp"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E9E9E9"/>
    <!--  摇一摇  -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:clickable="true"
        android:background="@drawable/bg_all">

        <ImageView
            android:id="@+id/find_shake"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/bg_find_shake" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:layout_toRightOf="@id/find_shake"
            android:text="摇一摇"
            android:textColor="@color/black"
            android:textSize="19dp" />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:src="@drawable/arrows" />

    </RelativeLayout>
    <View
        android:id="@+id/thickLine2"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#E9E9E9"/>
    <!--  看一看  -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:clickable="true"
        android:background="@drawable/bg_all">

        <ImageView
            android:id="@+id/find_look"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/bg_find_look" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:layout_toRightOf="@id/find_look"
            android:text="看一看"
            android:textColor="@color/black"
            android:textSize="19dp" />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:src="@drawable/arrows" />

    </RelativeLayout>
    <View
        android:layout_marginLeft="65dp"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E9E9E9"/>
    <!-- 搜一搜   -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:clickable="true"
        android:background="@drawable/bg_all">

        <ImageView
            android:id="@+id/find_search"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/bg_find_search" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:layout_toRightOf="@id/find_search"
            android:text="搜一搜"
            android:textColor="@color/black"
            android:textSize="19dp" />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:src="@drawable/arrows" />

    </RelativeLayout>
    <View
        android:id="@+id/thickLine3"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#E9E9E9"/>
    <!--  直播和附近  -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:clickable="true"
        android:background="@drawable/bg_all">

        <ImageView
            android:id="@+id/find_near"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/bg_find_near" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:layout_toRightOf="@id/find_near"
            android:text="直播和附近"
            android:textColor="@color/black"
            android:textSize="19dp" />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:src="@drawable/arrows" />

    </RelativeLayout>
    <View
        android:id="@+id/thickLine4"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#E9E9E9"/>
    <!--  购物  -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:clickable="true"
        android:background="@drawable/bg_all">

        <ImageView
            android:id="@+id/find_shop"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/bg_find_shop" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:layout_toRightOf="@id/find_shop"
            android:text="购物"
            android:textColor="@color/black"
            android:textSize="19dp" />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:src="@drawable/arrows" />

    </RelativeLayout>
    <View
        android:layout_marginLeft="65dp"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E9E9E9"/>
    <!--  游戏  -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:clickable="true"
        android:background="@drawable/bg_all">

        <ImageView
            android:id="@+id/find_game"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/bg_find_game" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:layout_toRightOf="@id/find_game"
            android:text="游戏"
            android:textColor="@color/black"
            android:textSize="19dp" />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:src="@drawable/arrows" />

    </RelativeLayout>
    <View
        android:id="@+id/thickLine5"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#E9E9E9"/>
    <!--  小程序  -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:clickable="true"
        android:background="@drawable/bg_all">

        <ImageView
            android:id="@+id/find_applet"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/bg_find_applet" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="14dp"
            android:layout_toRightOf="@id/find_applet"
            android:text="小程序"
            android:textColor="@color/black"
            android:textSize="19dp" />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:src="@drawable/arrows" />

    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#E9E9E9"
        >
    </View>


</LinearLayout>